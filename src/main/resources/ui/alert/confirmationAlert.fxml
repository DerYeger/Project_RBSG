<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="container" alignment="BOTTOM_RIGHT" prefHeight="400.0" prefWidth="600.0" styleClass="container" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.se19.team_g.project_rbsg.alert.ConfirmationAlertController">
   <children>
      <Label fx:id="label" alignment="TOP_LEFT" text="Label" />
      <HBox alignment="CENTER_RIGHT" styleClass="button-container">
         <children>
            <Button fx:id="accept" mnemonicParsing="false" text="Button" />
            <Button fx:id="cancel" mnemonicParsing="false" text="Button" />
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../darkTheme.css" />
      <URL value="@alert.css" />
   </stylesheets>
</VBox>

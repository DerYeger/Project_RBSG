<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<TilePane prefColumns="2" prefRows="1" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@UnitDetailView.css"/>
        <URL value="@../dark-theme.css"/>
    </stylesheets>
    <styleClass>
        <String fx:value="root-background-color"/>
        <String fx:value="unitPropertyContainer"/>
    </styleClass>
    <VBox styleClass="box">
        <Label fx:id="propertyValue" text="20"/>
    </VBox>
    <VBox fx:id="propertyIcon" styleClass="box"/>
</TilePane>

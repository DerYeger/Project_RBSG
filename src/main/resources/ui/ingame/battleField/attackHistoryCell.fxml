<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.se19.team_g.project_rbsg.ingame.battlefield.history.AttackHistoryCellController">
   <children>
      <ImageView fx:id="primaryIcon" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" />
      <StackPane>
         <children>
            <ImageView fx:id="secondaryIcon" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" />
            <Label fx:id="healthValue" contentDisplay="CENTER" text="0" textAlignment="CENTER" textFill="RED">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
         </children>
      </StackPane>
   </children>
</HBox>

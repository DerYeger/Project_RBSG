<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="CENTER" type="VBox" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.se19.team_g.project_rbsg.ingame.battlefield.history.HistoryViewController">
 <children>
      <Button fx:id="liveButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#getLive" styleClass="liveButton" text="Live">
         <VBox.margin>
            <Insets bottom="10.0" right="5.0" />
         </VBox.margin>
      </Button>
        <ListView fx:id="historyView" fixedCellSize="50.0" focusTraversable="false" maxWidth="120.0" minWidth="120.0" VBox.vgrow="SOMETIMES">
         <styleClass>
            <String fx:value="history" />
            <String fx:value="striped" />
            <String fx:value="arrowsHidden" />
             <String fx:value="bigScroll" />
         </styleClass></ListView>
    </children>
</fx:root>

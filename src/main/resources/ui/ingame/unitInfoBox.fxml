<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <HBox>
               <children>
                  <VBox fx:id="firstPropertyContainer">
                     <children>
                        <StackPane>
                           <children>
                              <Pane styleClass="elevation-8" />
                              <ImageView fx:id="unitImageView" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../assets/unit/portrait/soldier.gif" />
                                 </image>
                              </ImageView>
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
                  <VBox fx:id="secondPropertyContainer" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox fx:id="thirdPropertyContainer" />
   </children>
   <stylesheets>
      <URL value="@../darkTheme.css" />
      <URL value="@unitInfoBox.css" />
   </stylesheets>
   <styleClass>
      <String fx:value="elevation-2" />
      <String fx:value="unitInfoContainer" />
   </styleClass>
</HBox>

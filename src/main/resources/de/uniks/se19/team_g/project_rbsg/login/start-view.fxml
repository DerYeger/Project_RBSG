<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>

<AnchorPane fx:id="root" stylesheets="@start.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.se19.team_g.project_rbsg.login.StartViewController">
   <children>
       <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
             <children>
                <VBox fx:id="loginAndTitleBox" StackPane.alignment="CENTER" alignment="CENTER"/>
             </children>
       </StackPane>
       <Button mnemonicParsing="false" onAction="#toggleSound" AnchorPane.leftAnchor="10" AnchorPane.topAnchor="10">
           <styleClass>
               <String fx:value="iconButton" />
               <String fx:value="primary-background-color" />
               <String fx:value="floating-shadow" />
           </styleClass>
           <graphic>
               <ImageView fx:id="musicImage" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true"/>
           </graphic>
       </Button>
   </children>
</AnchorPane>
